{
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^react", "^@mui/(.*)$", "^@?\\w", "^[./]"],
  "semi": true,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "arrowParens": "always"
}
